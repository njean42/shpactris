script/source = "extends Node2D

var in_pause = false


func _input(event):
	if $'/root/world/HUD/game-over-menu'.visible:
		return
	
	if Input.is_action_pressed('ui_pause'):
		pause()
	
	if Input.is_action_just_released('ui_pause'):
		in_pause = get_tree().is_paused()

func pause(force=false):
	# go to pause
	if not in_pause and not get_tree().is_paused():
		visible = true
		get_tree().set_pause(true)
	
	# resume game
	if in_pause and get_tree().is_paused():
		visible = false
		get_tree().set_pause(false)
		if force:
			in_pause = get_tree().is_paused()
"

